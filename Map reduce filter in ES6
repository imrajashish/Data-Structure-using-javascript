arr = [1,2,3,4,5,6]
function double(x){
    return x*2
}
const output = arr.map(double)
console.log(output);

const arr1 = [12,22,33,322]
function isodd(y){
    return y%2
}
const even = arr.filter(isodd)
console.log(even);

const arr2 = [1,2,3,413e3]
function sumofarr(arr2){
    sum = 0;
    for(let x=0;x<arr2.length;x++){
        sum = sum + arr2[x];
    }
    return sum
}
 console.log(sumofarr(arr2))
